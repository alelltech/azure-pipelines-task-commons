{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./dist",
    "sourceRoot": "./src/",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "declaration": true,
    "esModuleInterop": true,
    // "strict": true,
    "strictNullChecks": false,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts"],
  "exclude": ["**/tests"]
}
